@{
    ViewBag.Title = "Starch Web Service HMI";
}
<body>
    <div id="wrapper">
        @Html.Partial("_LayoutNavbar")
        <div id="page-wrapper">
            <div class="container-fluid">
                <!-- /.row -->
                <div class="row">
                    <div class="col-lg-12">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <b class="panel-unit" align="left "><span class="glyphicon glyphicon-plus-sign" aria-hidden="true"></span> Fine Extractor: FEX34 </b>

                                <div style="display: inline-block;">
                                    <button style="margin-left: 15px;" onclick="location.href='@Url.Action("FEX01", "Fex")'; return false;" type="button" class="btn btn-secondary"> FEX01 </button>
                                    <button onclick="location.href='@Url.Action("FEX02", "Fex")'; return false;" type="button" class="btn btn-secondary "> FEX02 </button>
                                    <button class="btn btn-secondary disabled"> FEX34 </button>
                                    <button onclick="location.href='@Url.Action("FEXOver", "Fex")'; return false;" type="button" class="btn btn-primary active"> FEX: Overview </button>
                                </div>
                                <div style="display: inline-block; float: right;">
                                    <button onclick="location.href='@Url.Action("Index", "ProcessControl")'; return false;" type="button" class="btn btn-success"><span class="glyphicon glyphicon-cog" aria-hidden="true"></span> Controller </button>
                                    <button onclick="location.href='@Url.Action("Dashboard", "Dashboard")'; return false;" type="button" class="btn btn-warning"><i class="fa fa-fw fa-bar-chart-o"></i> Dashboard </button>
                                    
                                </div>




                            </div>
                            <div class="panel-body">
                                <div>
                                    <object id="svg_fex" data="/Content/FEX/FEX34z.svg" type="image/svg+xml" />
                                    <object id="svg_overview" data="/Content/OVERVIEW/Overviews.svg" type="image/svg+xml" />
                                </div>
                                
                                
                                
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container-fluid -->
    </div>
    <!-- /#page-wrapper -->
    
    <!-- /#wrapper -->
</body>
@using StarchServiceHMI.Models
<!-- jQuery -->
<script src="/js/jquery.js"></script>
<!-- Bootstrap Core JavaScript -->
<script src="/js/bootstrap.min.js"></script>
<!-- Morris Charts JavaScript -->
<script src="/js/plugins/morris/raphael.min.js"></script>
<script src="/js/plugins/morris/morris.min.js"></script>
<script src="/js/plugins/morris/morris-data.js"></script>
<script>
            $(window).load(function () {
                var height = $(window).height() - 50; // New height
                $("#page-wrapper").css("height", height);
            });

            $(window).resize(function () {
                var height = $(window).height() - 50; // New height
                $("#page-wrapper").css("height", height);
            });
</script>
<script>

    if (typeof (EventSource) !== "undefined") {
        var tagname = "?tag=" + "@(((List<TagConfig>)ViewData["TagConfigObject"]).Find(x => x.Id == 84).Tag_name)";
        var source = new EventSource('@Url.Action("Index", "Distributer")' + tagname);
        source.onmessage = function (event) {
            console.log(event);
            document.getElementById("svg_fex").contentDocument.getElementById("YieldFex34").innerHTML = "Yield : " + parseFloat(event.data).toFixed(3);
        };
    } else {
        document.getElementById("FiEx34").innerHTML = "Sorry, your browser does not support server-sent events...";
    }

    if (typeof (EventSource) !== "undefined") {
        var tagname = "?tag=" + "@(((List<TagConfig>)ViewData["TagConfigObject"]).Find(x => x.Id == 85).Tag_name)";
        var source = new EventSource('@Url.Action("Index", "Distributer")' + tagname);
        source.onmessage = function (event) {
            console.log(event);
            document.getElementById("svg_fex").contentDocument.getElementById("EMFFex34").innerHTML = "EMF : " + parseFloat(event.data).toFixed(3);
        };
    } else {
        document.getElementById("FiEx34").innerHTML = "Sorry, your browser does not support server-sent events...";
    }
    if (typeof (EventSource) !== "undefined") {
        var tagname = "?tag=" + "@(((List<TagConfig>)ViewData["TagConfigObject"]).Find(x => x.Id == 86).Tag_name)";
        var source = new EventSource('@Url.Action("Index", "Distributer")' + tagname);
        source.onmessage = function (event) {
            var elm = document.getElementById("svg_fex").contentDocument.getElementById("status-light");
            //if (event.data !== isNaN(true))
            $(elm).css("fill", 'rgb(102, 102, 101)');
            console.log(event.data + "   ...")
            if (event.data == "True")
                $(elm).css("fill", "#39b339");
            else if (event.data == "False")
                $(elm).css("fill", 'rgb(102, 102, 101)');
            else
                $(elm).css("fill", "red");
        };
    } else {
        document.getElementById("FiEx34").innerHTML = "Sorry, your browser does not support server-sent events...";
    }

    if (typeof (EventSource) !== "undefined") {
        var tagname = "?tag=" + "@(((List<TagConfig>)ViewData["TagConfigObject"]).Find(x => x.Id == 87).Tag_name)";
        var source = new EventSource('@Url.Action("Index", "Distributer")' + tagname);
        source.onmessage = function (event) {
            var elm = document.getElementById("svg_fex").contentDocument.getElementById("pump1-light");
            //if (event.data !== isNaN(true))
            $(elm).css("fill", 'rgb(102, 102, 101)');
            console.log(event.data + "   ...")
            if (event.data == "True")
                $(elm).css("fill", "#39b339");
            else if (event.data == "False")
                $(elm).css("fill", "red");

        };
    } else {
        document.getElementById("FiEx02").innerHTML = "Sorry, your browser does not support server-sent events...";
    }
</script>